%[entry]
fn main() {
    let x = mul(3, 5)
    XXXprint x

    mut a, b = 10, 20
    XXXprint a
    XXXprint b

    swap(&mut a, &mut b)
    XXXprint a
    XXXprint b

    loops()
}

fn mul(a: Int, b: Int): Int {
    return a * b
}

fn swap(a: &mut Int, b: &mut Int) {
    let t = *a
    *a = *b
    *b = t
}

fn loops() {
    for i = 0, i < 5 {
        i = i + 1
        XXXprint i
    }
}
